<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE swatch SYSTEM "swatch.dtd">

<swatch name="WORDPRESS_LINK_NORM">
    <!-- Base Satellite definition -->
    <sat shape="edgeShp" transfo="noTrf" isBack="true">
        <slice inCol="179 217 255" outCol="238 242 245"/>
    </sat>

    <!-- General purpose Selection -->
    <!-- Sel sats must be declared before the ones they should hides when they'll be activated -->
    <sat shape="edgeShp" linkDarkCol="128 0 0" linkNormCol="255 0 0" linkLitCol="255 143 143" selection="search" transfo="noTrf">
        <slice inCol="255 0 0" />
    </sat>

    <!-- References -->
    <shape id="edgeShp" scale="1" />
    <shape id="voidShp"/>

    <transfo id="selTrf" map="interpol">
        <polar dir="0" rad="1.4"/>
    </transfo>
    <transfo id="baseTrf" map="interpol">
        <polar dir="90"	rad="1"/>
    </transfo>

    <font id="selFnt" name="Helvetica" size="10"/>
    <transfo id="noTrf"/>
</swatch>
